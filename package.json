{
  "name": "tray",
  "version": "0.1.2",
  "description": "Textile Tray App",
  "main": "public/electron.ts",
  "homepage": "./",
  "author": "Textile",
  "license": "MIT",
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@textile/js-http-client": "0.2.18",
    "@textile/js-wallet": "^0.1.1",
    "@types/react-qr-reader": "^2.1.2",
    "@types/url-parse": "^1.4.3",
    "@types/zxcvbn": "^4.4.0",
    "electron": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-moment": "^0.8.4",
    "react-qr-reader": "^2.2.1",
    "react-reveal": "^1.2.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.85.0",
    "url-parse": "^1.4.7",
    "zxcvbn": "^4.4.2"
  },
  "scripts": {
    "credit": "node ./scripts/contributors.js",
    "lint": "./node_modules/tslint/bin/tslint  -c tslint.json --project .",
    "start": "craco start",
    "dev": "concurrently \"cross-env BROWSER=none yarn craco start\" \"wait-on http://localhost:3000\"",
    "predist": "yarn build",
    "dist": "./scripts/bundle.sh",
    "build": "craco build",
    "postbuild": "rimraf resources & move-cli build resources",
    "precommit": "yarn lint",
    "preversion": "yarn credit; yarn precommit",
    "release": "yarn version --patch",
    "postversion": "git push --follow-tags",
    "test": "craco test",
    "clean": "rimraf build resources node_modules output bind_*.go",
    "prelatest": "yarn install",
    "latest": "yarn upgrade @textile/js-http-client; go get github.com/textileio/go-textile"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "keywords": [
    "textile",
    "ipfs",
    "tray",
    "dweb",
    "p2p"
  ],
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@craco/craco": "^3.5.0",
    "@textile/tslint-rules": "^1.0.3",
    "@types/electron": "^1.6.10",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "babel-preset-mobx": "^2.0.0",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron-devtools-installer": "^2.2.4",
    "markdown-magic": "^0.1.25",
    "markdown-magic-github-contributors": "^0.0.3",
    "move-cli": "^1.2.1",
    "react-scripts": "2.1.5",
    "rimraf": "^2.6.3",
    "tslint": "^5.15.0",
    "typescript": "^3.3.4000",
    "wait-on": "^3.2.0"
  }
}
